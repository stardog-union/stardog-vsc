{
  "name": "stardog-rdf-grammars",
  "displayName": "Stardog RDF Grammars",
  "description": "Syntax highlighting for SPARQL, Turtle, Stardog Rules Syntax, and other RDF languages",
  "version": "0.0.1",
  "publisher": "stardog-union",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.5",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "main": "./out/src/extension",
  "activationEvents": [
    "onLanguage:sparql",
    "onLanguage:turtle",
    "onLanguage:stardog-mapping-syntax",
    "onLanguage:stardog-rules-syntax"
  ],
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sparql",
        "aliases": [
          "SPARQL",
          "sparql"
        ],
        "extensions": [
          ".rq",
          ".sparql"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "turtle",
        "aliases": [
          "TURTLE",
          "turtle"
        ],
        "extensions": [
          ".ttl",
          ".turtle"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "stardog-mapping-syntax",
        "aliases": [
          "Stardog Mapping Syntax"
        ],
        "extensions": [
          ".sms"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "stardog-rules-syntax",
        "aliases": [
          "Stardog Rules Syntax"
        ],
        "extensions": [
          ".srs"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sparql",
        "scopeName": "source.sparql",
        "path": "./syntaxes/sparql.tmLanguage.json"
      },
      {
        "language": "turtle",
        "scopeName": "source.turtle",
        "path": "./syntaxes/turtle.tmLanguage.json"
      },
      {
        "language": "stardog-mapping-syntax",
        "scopeName": "source.sms",
        "path": "./syntaxes/stardog-mapping.tmLanguage.json"
      },
      {
        "language": "stardog-rules-syntax",
        "scopeName": "source.srs",
        "path": "./syntaxes/stardog-rules.tmLanguage.json"
      }
    ]
  }
}